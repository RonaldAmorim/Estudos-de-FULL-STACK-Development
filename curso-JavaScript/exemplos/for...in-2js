//Formas de acessar a propriedade de um objeto
    console.log(pessoa['nome']) //cuidado n√£o tem '.' ponto entre o objeto e sua propriedade
    console.log(pessoa.nome) // agora aqui tem '.' ponto entre o objeto e a sua propriedade

console.log(`__________`)
    // Para imprimir cada valor de dentro de cada propriedade dentro do array pesquisado
    for (let i in pessoa) {
        console.log(pessoa[i])
    }